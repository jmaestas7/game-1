<!DOCTYPE html>
<html>
<head>
    <!-- Added link to the jQuery Library -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

   <!-- Added a link to Bootstrap-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

   <meta charset="UTF-8">
    <title>jQuery In Class Activity</title>
</head>

<body>

<h2 id="header">Try to guess the computer's random number it chose</h2>
<br>
<br>
<div id="buttons"></div>



<script type="text/javascript">

    $(document).ready(function() {
        var numList = [1, 2, 3, 4];
        chosenNumber = numList[Math.floor(Math.random() * numList.length)];
        console.log(chosenNumber);

        for(var i = 0; i < numList.length; i++) {
            var numBtn = $("<button>");
            numBtn.addClass("number-button");
            numBtn.attr("value", numList[i]);
            numBtn.text(numList[i]);
            $("#buttons").append(numBtn);
        }


        $(".number-button").on("click", function() {
            var userChoice = ($(this).attr("value"));
            userChoice = parseInt(userChoice);
            console.log(userChoice);
            if(userChoice == chosenNumber) {
                alert("You guessed correctly!");
                chosenNumber = numList[Math.floor(Math.random() * numList.length)];
            }
            else {
                alert("Try again!");
            }
        });
    });

</script>
</body>
</html>